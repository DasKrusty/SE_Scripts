@Launch
{
    OnOff_On DDR-3 Light
    Color of DDR-3 Light = 55:155:255
    Blink Interval of DDR-3 Light = 1
    Discharge DDR-3 Battery
    OnOff_On DDR-3 Thruster
    OnOff_On DDR-3 Gyroscope
    Writenew to DDR-3 Debug LCD = Flight Systems up
    Delay 1000
    OnOff_On DDR-3 Antenna
    Writeline to DDR-3 Debug LCD = Comm Systems up
    Delay 1000
    Writeline to DDR-3 Debug LCD = Releasing Drone
    Delay 1000
    Unlock DDR-3 Connector
    Delay 1000
    ID_PLAY_CHECKBOX of DDR-3 AI Recorder (Launch) = True
    Color of DDR-3 Light Spot = 255:0:0
    Blink Interval of DDR-3 Light Spot = 0
    Color of DDR-3 Light Nav R = 0:255:0
    Color of DDR-3 Light Nav L = 255:0:0
}

@Launch2
{
    OnOff_On DDR-3 Light
    Delay 1000
    Color of DDR-3 Light = 55:155:255
    Blink Interval of DDR-3 Light = 1
    ActivateBehavior of DDR-3 AI Basic (Task) = True
    Delay 1000
    ActivateBehavior of DDR-3 AI Offensive (Combat) = True
    Delay 1000
    SpeedLimit of ActivateBehavior = 100
    Color of DDR-3 Light Spot = 255:0:0
    Blink Interval of DDR-3 Light Spot = 0
    Color of DDR-3 Light Nav R = 0:255:0
    Color of DDR-3 Light Nav L = 255:0:0
}

@Land
{
    OnOff_On DDR-3 Light
    Color of DDR-3 Light = 55:155:255
    Blink Interval of DDR-3 Light = 1
    Delay 2000
    Writenew to DDR-3 Debug LCD = Powering off Thrusters
    Delay 1000
    ActivateBehavior of DDR-3 AI Offensive (Combat) = False
    OnOff_Off DDR-3 Thruster
    Delay 3000
    When connected of DDR-3 Connector = Connectable 
    {
        Writeline to DDR-3 Debug LCD = Connecting
        Delay 1000
        Lock DDR-3 Connector
        Recharge DDR-3 Battery
        OnOff_Off DDR-3 Gyroscope
        OnOff_Off DDR-3 Light
        OnOff_Off DDR-3 Antenna
        Writeline to DDR-3 Debug LCD = Connected
    }
}

