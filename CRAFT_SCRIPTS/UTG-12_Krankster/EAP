@park{
    Color of \LIST = 255:0:0
    Write to \LCIN = P
    Rotate \HIWA to 0 at 1
    * strength of hinge - math
    * steering angle of Wheel
    * speed of wheel / and handbrake
}

@gear_1{
    Color of \LIST = 255:255:0
    Write to \LCIN = 1
    Rotate \HIWA to 45 at 1
}

@gear_2{
    Color of \LIST = 255:200:0
    Write to \LCIN = 2
    Rotate \HIWA to 40 at 1
}

